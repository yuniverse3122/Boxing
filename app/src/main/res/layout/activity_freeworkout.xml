<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:contentDescription="@string/free_workout"
    tools:context=".Freeworkout">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="670dp"
        android:layout_height="769dp"
        app:srcCompat="@drawable/work"
        tools:layout_editor_absoluteX="-130dp"
        tools:layout_editor_absoluteY="-30dp" />

    <Button
        android:id="@+id/History"
        android:layout_width="289dp"
        android:layout_height="130dp"
        android:layout_marginBottom="106dp"

        android:background="@drawable/custom_button"
        android:text="@string/advanced"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="0.703" />

    <Button
        android:id="@+id/button2"
        android:layout_width="289dp"
        android:layout_height="130dp"
        android:layout_marginTop="338dp"
        android:layout_marginBottom="263dp"

        android:background="@drawable/custom_button"
        android:text="@string/intermediate"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="210dp"
        android:layout_height="63dp"
        android:layout_marginTop="36dp"
        android:autofillHints=""
        android:background="@drawable/custom_button"
        android:ems="10"
        android:hint="@string/empty_space"
        android:inputType="textPersonName"
        android:text="@string/free_workout"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/Pre_selected"
        android:layout_width="289dp"
        android:layout_height="130dp"
        android:layout_marginTop="179dp"
        android:layout_marginBottom="422dp"
        android:background="@drawable/custom_button"

        android:text="@string/beginner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>